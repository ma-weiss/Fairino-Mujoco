<?xml version="1.0" encoding="utf-8"?>
<!-- MuJoCo MJCF converted from fr5_robot.urdf
     Validate: python -c "import mujoco; mujoco.MjModel.from_xml_path('fr5_robot.xml')"
     Run from this file's directory so meshes/ resolves. -->
<mujoco model="fr5_robot">
  <compiler angle="radian" meshdir="meshes"/>

  <option gravity="0 0 -9.81"/>

  <default>
    <joint damping="0"/>
  </default>

  <asset>
    <mesh name="base_link_mesh" file="base_link.STL"/>
    <mesh name="shoulder_link_mesh" file="shoulder_link.STL"/>
    <mesh name="upperarm_link_mesh" file="upperarm_link.STL"/>
    <mesh name="forearm_link_mesh" file="forearm_link.STL"/>
    <mesh name="wrist1_link_mesh" file="wrist1_link.STL"/>
    <mesh name="wrist2_link_mesh" file="wrist2_link.STL"/>
    <mesh name="wrist3_link_mesh" file="wrist3_link.STL"/>
  </asset>

  <worldbody>
    <body name="base_link" pos="0 0 0">
      <inertial pos="-0.00044457 0.0004811 0.032592" mass="1.073"
               fullinertia="0.0024567 0.0024575 0.0038879 -1.5435E-05 -1.3507E-05 1.4421E-05"/>
      <geom name="base_link_geom" type="mesh" mesh="base_link_mesh" rgba="0.898 0.918 0.929 1"/>
      <!-- shoulder_pan: revolute base_link -> shoulder_Link -->
      <body name="shoulder_link" pos="0 0 0" euler="0 0 0">
        <inertial pos="1.3785E-06 -0.0031495 0.14526" mass="4.1676"
                 fullinertia="0.010397 0.010278 0.0076307 -1.005E-08 1.6274E-07 0.00015856"/>
        <joint name="shoulder_pan" type="hinge" axis="0 0 1" range="-3.0543261 3.0543261"/>
        <geom name="shoulder_link_geom" type="mesh" mesh="shoulder_link_mesh" rgba="0.898 0.918 0.929 1" euler="-1.570796 1.570796 0" pos="0 0 0.09"/>
        <!-- shoulder_lift: revolute shoulder_link -> upperarm_link -->
        <body name="upperarm_link" pos="0 0 0.155" euler="1.570796 -1.570796 0">
          <inertial pos="-2.2928E-05 0.33278 0.084237" mass="20.782"
                   fullinertia="2.9074 0.12653 2.8157 -0.00023104 -2.5049E-05 -0.1479"/>
          <joint name="shoulder_lift" type="hinge" axis="0 0 1" range="-4.6251225 1.4835298"/>
          <geom name="upperarm_link_geom" type="mesh" mesh="upperarm_link_mesh" rgba="0.898 0.918 0.929 1"/>
          <!-- elbow: revolute upperarm_link -> forearm_link -->
          <body name="forearm_link" pos="0 0.425 0" euler="0 0 -1.570796">
            <inertial pos="0.092224 -2.2928E-05 0.084237" mass="20.782"
                     fullinertia="0.12653 2.9074 2.8157 0.00023104 0.1479 -2.5049E-05"/>
            <joint name="elbow" type="hinge" axis="0 0 1" range="-2.8274333 2.8274333"/>
            <geom name="forearm_link_geom" type="mesh" mesh="forearm_link_mesh" rgba="0.898 0.918 0.929 1"/>
            <!-- wrist_1: revolute forearm_link -> wrist1_link -->
            <body name="wrist1_link" pos="-0.395 0 0" euler="0 0 1.570796">
              <inertial pos="0.48722 -2.2928E-05 0.084237" mass="20.782"
                       fullinertia="0.12653 2.9074 2.8157 0.00023104 0.1479 -2.5049E-05"/>
              <joint name="wrist_1" type="hinge" axis="0 0 1" range="-4.6251225 1.4835298"/>
              <geom name="wrist1_link_geom" type="mesh" mesh="wrist1_link_mesh" rgba="0.898 0.918 0.929 1" euler="0 0 1.570796"/>
              <!-- wrist_2: revolute wrist1_link -> wrist2_link -->
              <body name="wrist2_link" pos="0 0 0.13" euler="1.570796 0 -1.570796">
                <!-- URDF had mass=1.570796 (pi/2); using 0.5 kg as plausible value -->
                <inertial pos="-2.396E-06 0.0045906 0.099342" mass="0.5"
                         fullinertia="0.0022393 0.0013914 0.0021864 -1.0022E-07 -3.2115E-08 -2.2223E-05"/>
                <joint name="wrist_2" type="hinge" axis="0 0 1" range="-3.0543261 3.0543261"/>
                <geom name="wrist2_link_geom" type="mesh" mesh="wrist2_link_mesh" rgba="0.898 0.918 0.929 1"  euler="0 0 1.570796"/>
                <!-- wrist_3: revolute wrist2_link -> wrist3_link -->
                <body name="wrist3_link" pos="0 0 0.102" euler="-1.570796 1.570796 0">
                  <inertial pos="5.8427E-06 7.5807E-05 0.079126" mass="0.52527"
                           fullinertia="0.00027801 0.00027736 0.00041582 -3.5346E-07 -1.7431E-07 -7.5888E-08"/>
                  <joint name="wrist_3" type="hinge" axis="0 0 1" range="-3.0543261 3.0543261"/>
                  <geom name="wrist3_link_geom" type="mesh" mesh="wrist3_link_mesh" rgba="0.898 0.918 0.929 1"/>
                  <!-- arm_hand_joint: fixed wrist3_link -> hand_base_link -->
                  <site name="attachment_site" pos="0 0 0.102" size="0.01" rgba="1 0 0 1"/>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <motor name="shoulder_pan" joint="shoulder_pan" ctrllimited="true" ctrlrange="-3.0543261 3.0543261"/>
    <motor name="shoulder_lift" joint="shoulder_lift" ctrllimited="true" ctrlrange="-4.6251225 1.4835298"/>
    <motor name="elbow" joint="elbow" ctrllimited="true" ctrlrange="-2.8274333 2.8274333"/>
    <motor name="wrist_1" joint="wrist_1" ctrllimited="true" ctrlrange="-4.6251225 1.4835298"/>
    <motor name="wrist_2" joint="wrist_2" ctrllimited="true" ctrlrange="-3.0543261 3.0543261"/>
    <motor name="wrist_3" joint="wrist_3" ctrllimited="true" ctrlrange="-3.0543261 3.0543261"/>
  </actuator>

  <keyframe>
    <key name="home" qpos="0 0 0 -1.57079 0 -0.7853" ctrl="0 0 0 0 0 0"/>
  </keyframe>
</mujoco>
